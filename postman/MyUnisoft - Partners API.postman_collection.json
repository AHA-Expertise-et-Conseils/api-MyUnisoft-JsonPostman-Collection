	"event": [
		{
			"listen": "prerequest",
			"script": {
				"type": "text/javascript",
				"exec": [
					"const auth_baseurl = pm.environment.get('auth_baseurl');",
					"const mail = pm.environment.get('USER_EMAIL');",
					"const password = pm.environment.get('USER_PASSWORD');",
					"",
					"pm.sendRequest({",
					"    url: auth_baseurl + \"/authenticate\",",
					"    method: 'POST',",
					"    header: {",
					"        'content-type': 'application/json'",
					"    },",
					"    body: {",
					"        mode: 'raw',",
					"        raw: JSON.stringify({",
					"            \"password\": password,",
					"            \"mail\": mail",
					"        })",
					"    }",
					"}, (err, res) => {",
					"    const result = res.json();",
					"    pm.environment.set(\"TOKEN\", result.details.access_token);",
					"});",
					""
				]
			}
		},
